public class Main extends Thread
{
    
    public void run()
    {
        int n=2;
        for(int i=1;i<=10;i++)
        {
            System.out.println("2"+"*"+i+"="+n*i);
        }
        
    }
	public static void main(String[] args) 
	{
	    Main t1=new Main();
	    
	    Thread object=new Thread(t1);
	    object.start();
	     
	    
		//System.out.println("this code is outside of a thread");
	}
}
