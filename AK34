public class Main
{
	public static void main(String[] args) 
	{
	    //create a string
	    String greet="Hello! World"; //greet is object as String is class
	    System.out.println("String:"+greet);
	    //get length of greet
	    int length=greet.length(); 
	    System.out.println("Length:"+length);
	}
}
