//package errorhandling;
public class error
{
    public static void main(String []args)
    {
        try
        {
            int x=10,y=0;
            int z=x/y;
            
            System.out.println(z);
            
        }
        catch(Exception e)
        {
            System.out.println("Not possible ---->"+e);
        }
    }
}
