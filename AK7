
import java.util.Scanner;
class A 
{
    public static void main(String[] args) 
    {
        int i,gcd=1;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter an integer : ");
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        
        for( i=1;i<=n1&&i<=n2;i++)
        {
            if(n1%i==0&n2%i==0)
            {
                gcd=i;
                
            }
        }
        
        
        System.out.println(gcd);
    }
}
