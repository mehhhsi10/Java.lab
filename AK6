
import java.util.Scanner;
class A 
{
    public static void main(String[] args) 
    {
        int f=1;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter an integer : ");
        int n=sc.nextInt();
        while(n!=0)
        {
            f=f*n--;
        }
        
        System.out.println("Factorial of is "+f);
    }
}
