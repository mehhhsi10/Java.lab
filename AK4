
import java.util.Scanner;
class A 
{
    public static void main(String[] args) 
    {
        int sum=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter an integer : ");
        int n=sc.nextInt();
        for(int i=1;i<=n;i++)
        {
            sum=sum+i;
        }
        
        System.out.println("Summation of all the elements from 1 to:"+n+" is "+sum);
    }
}
